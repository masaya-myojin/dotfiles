[user]
  name = Masaya Myojin
  email = m.myojin0425@gmail.com
[core]
  excludesfile = /Users/mmyoji/.gitignore_global
  editor = vi
[push]
  default = simple
[alias]
  a = add
  aa = add --all
  b = branch
  co = checkout
  cob = checkout -b
  cma = commit --amend
  com = commit
  d = diff --compaction-heuristic
  show-conflicted = diff --name-only --diff-filter=U
  fc = fetch --prune
  l = log --graph
  ps = push -u
  s = status
  pl = !"git pull && git fetch --prune"
  vtree = log --pretty=oneline --graph --decorate --all
