[user]
  name = Masaya Myojin
  email = m.myojin0425@gmail.com
[core]
  editor = vi
[push]
  default = simple
[alias]
  a = add
  aa = add --all
  b = branch
  co = checkout
  cob = checkout -b
  cma = commit --amend
  com = commit
  d = diff --compaction-heuristic
  show-conflicted = diff --name-only --diff-filter=U
  l = log --graph
  ps = push -u
  s = status
  vtree = log --pretty=oneline --graph --decorate --all

  search = log -p -S
[rebase]
  autosquash = true
[fetch]
  prune = true
